{{#if code}}
    {{#with (toLower code)}}
        {{#if (eq this 'active')}}
	        "in-progress"
        {{else}}
            {{code}}
        {{/if}}
    {{/with}}
{{/if}}

{{! values to be mapped: preparation | in-progress | not-done | on-hold | stopped | completed | entered-in-error | unknown}}
