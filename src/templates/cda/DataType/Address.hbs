{
    "use": {{>ValueSet/AddressUse.hbs code=addr.use}},
    "line": [
       	{{#each (toArray addr.streetAddressLine)}}
           	"{{{this._}}}",
       	{{/each}}
    ],
    "city": "{{addr.city._}}",
    "state": "{{>ValueSet/StateReference.hbs state=addr.state._}}",
    "country": "{{>ValueSet/CountryReference.hbs country=addr.country._}}",
    "postalCode": "{{addr.postalCode._}}",
},
